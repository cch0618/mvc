<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.test.signup.dao.ISignupDAO">

    <select id="insertSignup" resultType="com.test.signup.dto.SignupDTO">
        insert into signup(user_id, user_pwd ,user_name)
        values (#{user_id}, #{user_pwd}, #{user_name})
    </select>


</mapper>